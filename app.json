{
  "expo": {
    "name": "ignitegym",
    "slug": "ignitegym",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoRevitalia.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoRevitalia.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f0f0f"
      
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121214"
      }
    },
    "web": {
      "favicon": "./assets/logoRevitalia.png"
    }
  }
}
